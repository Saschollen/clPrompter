                <Value Val="*SQLSRC" MapTo="*SQLSRC"/>
                <Value Val="*SQLSCRIPT" MapTo="*SQLSCRIPT"/>
                <Value Val="*SQLIMMED" MapTo="*SQLIMMED"/>
                <Value Val="*DISPLAY" MapTo="*"/>
