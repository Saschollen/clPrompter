</Parm>
<Parm Kwd="CSVOPT" PosNbr="22" KeyParm="NO" PmtCtl="PMTCTL" ListDspl="INT2" RtnVal="NO" PassVal="DFT" Type="ELEM" Min="0" Max="1" Prompt="CSV Options" PromptMsgID="QFC0130" Rstd="NO" AlwUnprt="YES" AlwVar="YES" Expr="NO" Full="NO" DspInput="YES" IsDtaAra="NO" IsFile="NO" IsPgm="NO">
