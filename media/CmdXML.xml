</Parm>
<Parm Kwd="STMFHDR" PosNbr="21" KeyParm="NO" RtnVal="NO" PassVal="DFT" Type="LGL" Min="0" Max="1" Prompt="Insert Content-Type Header" Len="1" Rstd="YES" Dft="*NO" AlwUnprt="YES" AlwVar="YES" Expr="YES" Full="NO" DspInput="YES" Vary="NO" PassAtr="NO" Case="MONO" CCSID="JOB" IsDtaAra="NO" IsFile="NO" IsPgm="NO" Choice="*YES, *NO">
    <SpcVal>
        <Value Val="*YES" MapTo="1"/>
