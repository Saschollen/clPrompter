    <Elem Type="INT4" Min="0" Max="1" Prompt="Headings" PromptMsgID="QFC0131" Rstd="YES" Dft="*COLHDG" AlwUnprt="YES" AlwVar="YES" Expr="NO" Full="NO" DspInput="YES" Vary="NO" PassAtr="NO" Case="MONO" CCSID="JOB" IsDtaAra="NO" IsFile="NO" IsPgm="NO" Choice="*FLDNAME, *COLHDG, *NONE">
        <SpcVal>
            <Value Val="*NONE" MapTo="0"/>
