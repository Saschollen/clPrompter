    <Elem Type="CHAR" Min="0" Max="1" Prompt="Separator symbol" PromptMsgID="QFC0135" Len="10" Rstd="NO" Dft="*COMMA" AlwUnprt="YES" AlwVar="YES" Expr="YES" Full="NO" DspInput="YES" Vary="NO" PassAtr="NO" Case="MONO" CCSID="JOB" InlPmtLen="10" IsDtaAra="NO" IsFile="NO" IsPgm="NO" Choice="*COMMA, *TAB, char...">
        <SpcVal>
            <Value Val="*COMMA" MapTo=","/>
            <Value Val="*TAB" MapTo="X&apos;09&apos;"/>
