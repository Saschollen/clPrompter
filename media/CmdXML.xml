        </Parm>
        <Parm Kwd="AS" PosNbr="23" KeyParm="NO" PmtCtl="PMTRQS" RtnVal="NO" PassVal="DFT" Type="NAME" Min="0" Max="1" Prompt="Correlation name (file alias)" PromptMsgID="QFC0105" Len="32" Rstd="NO" Dft="*NONE" AlwUnprt="YES" AlwVar="YES" Expr="YES" Full="NO" DspInput="YES" Vary="INT2" PassAtr="NO" Case=â€¦ CmpVal="*XML"/>
