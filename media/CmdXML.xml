<QcdCLCmd DTDVersion="2.0">
    <Cmd CmdName="RUNIQRYF" CmdLib="IQUERY" CCSID="37" HlpPnlGrp="RUNIQRYF" HlpPnlGrpLib="IQUERY" HlpID="RUNIQRYF" MaxPos="99" PrdLib="IQUERY" Prompt="Run ad hoc iQuery" PromptMsgID="QFC0100" MsgF="COZLANGEN" MsgFLib="IQUERY" PmtFile="COZLANGEN" PmtFileLib="IQUERY" PmtFileMsg="DYNAMIC" ExecBatch="YES" ChgCmdExit="NO" RtvCmdExit="NO">
