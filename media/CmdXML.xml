            <Elem Type="QUAL" Min="1" Max="1" Prompt="Table" PromptMsgID="QFC0138" Rstd="NO" AlwUnprt="YES" AlwVar="YES" Expr="YES" Full="NO" DspInput="YES" IsDtaAra="NO" IsFile="NO" IsPgm="NO">
