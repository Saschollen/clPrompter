    <Elem Type="CHAR" Min="0" Max="1" Prompt="End of line symbol(s)" PromptMsgID="QFC0137" Len="10" Rstd="NO" Dft="*CRLF" AlwUnprt="YES" AlwVar="YES" Expr="NO" Full="NO" DspInput="YES" Vary="NO" PassAtr="NO" Case="MONO" CCSID="JOB" InlPmtLen="10" IsDtaAra="NO" IsFile="NO" IsPgm="NO" Choice="*CRLF, *CR, *LF, *LFCR, char..">
