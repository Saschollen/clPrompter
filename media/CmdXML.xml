        <Parm Kwd="OUTPUT" PosNbr="3" KeyParm="NO" RtnVal="NO" PassVal="DFT" Type="CHAR" Min="0" Max="1" Prompt="Output" PromptMsgID="QFC0104" Len="10" Rstd="YES" Dft="*" AlwUnprt="YES" AlwVar="YES" Expr="YES" Full="NO" DspInput="YES" Vary="NO" PassAtr="NO" Case="MONO" CCSID="JOB" IsDtaAra="NO" IsFile="NO" IsPgm="NO" Choice="*, *PRINT, *FILE, *PDF">
