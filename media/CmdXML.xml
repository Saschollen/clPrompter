            <Value Val="*COLHDG" MapTo="2"/>
            <Value Val="*FIELDS" MapTo="1"/>
            <Value Val="*NOHDG" MapTo="0"/>
            <Value Val="0" MapTo="0"/>
            <Value Val="1" MapTo="1"/>
