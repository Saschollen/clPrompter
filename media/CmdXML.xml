    <Elem Type="CHAR" Min="0" Max="1" Prompt="Quote symbol" PromptMsgID="QFC0133" Len="10" Rstd="NO" Dft="*QUOTE" AlwUnprt="YES" AlwVar="YES" Expr="YES" Full="NO" DspInput="YES" Vary="NO" PassAtr="NO" Case="MONO" CCSID="JOB" InlPmtLen="10" IsDtaAra="NO" IsFile="NO" IsPgm="NO" Choice="*QUOTE, *APOS, *BAR, *NONE...">
